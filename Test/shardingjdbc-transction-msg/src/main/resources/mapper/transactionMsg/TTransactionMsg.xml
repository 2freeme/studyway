<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.module.dao.TTransactionMsgMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.demo.module.pojo.TTransactionMsg">
        <id column="id" property="id" />
        <result column="sharding_column" property="shardingColumn" />
        <result column="batch_no" property="batchNo" />
        <result column="busi_class" property="busiClass" />
        <result column="busi_method" property="busiMethod" />
        <result column="mq_topic" property="mqTopic" />
        <result column="mq_tag" property="mqTag" />
        <result column="message" property="message" />
        <result column="status" property="status" />
        <result column="retry_times" property="retryTimes" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, sharding_column, batch_no, busi_class, busi_method, mq_topic, mq_tag, message, status, retry_times, create_time, update_time
    </sql>

</mapper>
